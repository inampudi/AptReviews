<section id="sample-review-container">
    <div id="sample-review">
    


<% @reviews.each do |review| %>
        <div id="sample-review-text">
            <p>
                <strong class="title"><%= review.title %></strong><br />
                by <%= review.nickname %> on September 25, 2012<br />
                <em>Currently lives here: Reviewer Type <%= review.reviewertype %> </em>
            </p>

            <p><%= review.reviewdesc %></p>
        </div>
        <div id="sample-review-rating">
            <div class="title">
                <div class="star-filled"></div><div class="star-filled"></div><div class="star-filled"></div><div class="star-filled"></div><div class="star-empty"></div> 
                4 of 5
            </div>
        </div>    
            <div class="rating-container">
                <div class="rating-criteria">Location/Neighborhood:</div>
                <div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-empty"></div>
                <div class="rating-numeric">4.0 of 5</div>
                <div class="clear"></div>
            </div>
            <div class="rating-container">
                <div class="rating-criteria">Overall Value:</div>
                <div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-empty"></div>
                <div class="rating-numeric">4.0 of 5</div>
                <div class="clear"></div>
            </div>
            <div class="rating-container">
                <div class="rating-criteria">Office Staff:</div>
                <div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-empty"></div>
                <div class="rating-numeric">4.0 of 5</div>
                <div class="clear"></div>
            </div>
            <div class="rating-container">
                <div class="rating-criteria">Maintenance Service:</div>
                <div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-empty"></div><div class="rating-empty"></div>
                <div class="rating-numeric">3.0 of 5</div>
                <div class="clear"></div>
            </div>
            <div class="rating-container">
                <div class="rating-criteria">Quality of Amenities:</div>
                <div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-empty"></div>
                <div class="rating-numeric">4.0 of 5</div>
                <div class="clear"></div>
            </div>
            <div class="rating-container">
                <div class="rating-criteria">Noise:</div>
                <div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-filled"></div><div class="rating-empty"></div>
                <div class="rating-numeric">4.0 of 5</div>
                <div class="clear"></div>
            </div>      
        </div>
        <div class="clear"></div>
    </div>

<% end %>

    <div class="clear"></div>
